    "@types/tmp": "^0.1.0",
    "@zeit/ncc": "^0.20.1",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "prisma2": "^0.0.95",
    "tmp": "^0.1.0",
    "ts-jest": "^24.0.2",
